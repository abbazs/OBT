{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "$ref": "#/definitions/Trade",
    "definitions": {
        "Trade": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "name": {
                    "type": "string"
                },
                "id": {
                    "type": "integer"
                },
                "user": {
                    "type": "string"
                },
                "created_on": {
                    "type": "string"
                },
                "active": {
                    "type": "boolean"
                },
                "PNL": {
                    "type": "integer"
                },
                "positions": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/Position"
                    }
                }
            },
            "required": [
                "PNL",
                "active",
                "created_on",
                "id",
                "name",
                "positions",
                "user"
            ],
            "title": "Trade"
        },
        "Position": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "index": {
                    "type": "integer"
                },
                "symbol": {
                    "type": "string"
                },
                "instrument": {
                    "type": "string"
                },
                "type": {
                    "type": "string"
                },
                "strike": {
                    "anyOf": [
                        {
                            "type": "integer"
                        },
                        {
                            "type": "null"
                        }
                    ]
                },
                "ST": {
                    "type": "string"
                },
                "ND": {
                    "type": "string"
                },
                "ED": {
                    "type": "string"
                },
                "TT": {
                    "type": "string"
                },
                "OPR": {
                    "type": "integer"
                },
                "CPR": {
                    "type": "integer"
                }
            },
            "required": [
                "ED",
                "OPR",
                "ST",
                "TT",
                "instrument",
                "strike",
                "symbol",
                "type"
            ],
            "title": "Position"
        }
    }
}