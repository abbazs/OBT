{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "$ref": "#/definitions/position",
    "definitions": {
        "position": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "name": {
                    "type": "string"
                },
                "symbol": {
                    "type": "string"
                },
                "position_entry_exits": {
                    "$ref": "#/definitions/pee"
                },
                "legs": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/leg"
                    }
                }
            },
            "required": [
                "name",
                "start_date",
                "symbol"
            ],
            "title": "position"
        },
        "ee": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "ENTRY": {
                    "price": {
                        "type": "number"
                    },
                    "date": {
                        "type": "integer"
                    }
                },
                "EXIT": {
                    "price": {
                        "type": "number"
                    },
                    "date": {
                        "type": "integer"
                    }
                }
            },
            "required": [],
            "title": "EntryAndExit"
        },
        "pee": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "vix": {
                    "$ref": "#/definitions/ee"
                },
                "iv": {
                    "$ref": "#/definitions/ee"
                },
                "spot": {
                    "$ref": "#/definitions/ee"
                }
            },
            "required": [
                "iv",
                "spot",
                "vix"
            ],
            "title": "PositionEntryExit"
        },
        "greek": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "vol": {
                    "$ref": "#/definitions/ee"
                },
                "theta": {
                    "$ref": "#/definitions/ee"
                },
                "delta": {
                    "$ref": "#/definitions/ee"
                },
                "gamma": {
                    "$ref": "#/definitions/ee"
                },
                "vega": {
                    "$ref": "#/definitions/ee"
                }
            },
            "required": [],
            "title": "greek"
        },
        "ees": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "vix": {
                    "$ref": "#/definitions/ee"
                },
                "iv": {
                    "$ref": "#/definitions/ee"
                },
                "greek": {
                    "$ref": "#/definitions/greek"
                },
                "price": {
                    "$ref": "#/definitions/ee"
                }
            },
            "required": [
                "price",
                "vix"
            ],
            "title": "LegEntryAndExits"
        },
        "leg": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "parent": {
                    "$ref": "#/definitions/position"
                },
                "start_date": {
                    "type": "integer"
                },
                "end_date": {
                    "type": "integer"
                },
                "expiry_date": {
                    "type": "integer"
                },
                "symbol": {
                    "type": "string"
                },
                "strike": {
                    "type": "number"
                },
                "entry_and_exits": {
                    "$ref": "#/definitions/ees"
                },
                "instrument": {
                    "type": "string"
                },
                "trade_type": {
                    "type": "string"
                }
            },
            "required": [
                "expiry_date",
                "entry_and_exits",
                "instrument",
                "parent",
                "start_date",
                "strike",
                "symbol",
                "trade_type"
            ],
            "title": "PositionLeg"
        }
    }
}